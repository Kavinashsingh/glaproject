#include<stdio.h>
#include<conio.h>
int main()
{
    printf("Hello Compiler, I am C");
    getch();  // holds output screen, until user press a key
    return 0;
}


#include<stdio.h>
#include<conio.h>
int main()
{
    char name[20];
    printf("Enter your Name: ");
    scanf("%s", name);
    printf("Hello, %s\nWelcome to codescracker.com", name);
    getch();
    return 0;
}



